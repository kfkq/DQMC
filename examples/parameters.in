[Lattice]
L1 = 4
L2 = 4

[hubbard]
U = 4.0                        # On-site interaction strength
t =  1.0                        # Hopping parameters
mu = -0.1                       # Chemical potential

[simulation]
beta = 5.0                      # Inverse temperature
nt = 50                        # Number of Imaginary time step
n_therms = 1_000                 # Number of thermalization sweeps
n_sweeps = 50                   # Number of Monte Carlo sweeps
n_bins = 200                    # Number of total bins. each bin contain average observable after n_sweeps 
n_stab = 9                      # Length of numerical stabilization in imaginary time propagation
symm = false
isMeasureUnequalTime = true     # whether to measure unequaltime
